{
  "expo": {
    "name": "foodAdmin",
    "slug": "foodAdmin",
    "scheme": "foodAdmin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconteste.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splashteste.png",
      "resizeMode": "contain",
      "backgroundColor": "#8a2be2"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
